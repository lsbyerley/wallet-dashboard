{
  "name": "wallet-dashboard",
  "scripts": {
    "runContract": "npx hardhat run scripts/run.js",
    "deployLocal": "npx hardhat run scripts/deploy.js --network localhost",
    "deployRinkeby": "npx hardhat run scripts/deploy.js --network rinkeby",
    "deployGoerli": "npx hardhat run scripts/deploy.js --network goerli",
    "deployMumbai": "npx hardhat run scripts/deploy.js --network mumbai",
    "deployArbitrumRinkeby": "npx hardhat run scripts/deploy.js --network arbitrumRinkeby",
    "deployArbitrumOne": "npx hardhat run scripts/deploy.js --network arbitrumOne",
    "pkg:update:check": "ncu",
    "pkg:update:install": "ncu -u; npm install",
    "pkg:update:int": "ncu -i"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.1.0",
    "@nomiclabs/hardhat-etherscan": "3.1.0",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@openzeppelin/contracts": "4.7.1",
    "chai": "4.3.6",
    "dotenv": "16.0.1",
    "ethereum-waffle": "3.4.4",
    "ethers": "5.6.9",
    "hardhat": "2.10.1",
    "typescript": "4.7.4",
    "zksync-web3": "0.7.9"
  }
}
